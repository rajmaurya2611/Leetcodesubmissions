class Solution {
    public int cal(int n, int dp[]) {
        if(n==1||n==2){
            return n;
        }
        if(dp[n]!=0){
            return dp[n];
        }
        dp[n]=cal(n-1,dp)+cal(n-2,dp);
        return dp[n];
    }
    public int climbStairs(int n){
        int dp[]=new int[n+1];
        return (cal(n, dp));
    }
}
